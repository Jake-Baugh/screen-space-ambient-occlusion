cbuffer SSAOBuffer : register(b10)
{
	float farClip;
	float nearClip;
	int kernal_size;
	int width;
	int height;
}

struct PixelIn
{
	float4 pos			: SV_POSITION;
	float2 tex			: TEXCOORD;
	float3 normal		: NORMAL;
};


float4 main(PixelIn input) : SV_TARGET
{ 
	return (float4)float4(input.normal, input.pos.z);
}